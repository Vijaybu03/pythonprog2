def calculator(a, b, operator):
    """Perform basic arithmetic operations."""
    if operator == '+':
        return a + b
    elif operator == '-':
        return a - b
    elif operator == '*':
        return a * b
    elif operator == '/':
        if b == 0:
            return "Error: Division by zero"
        return a / b
    else:
        return "Error: Invalid operator"
num1 = 10
num2 = 5
op = '*'
print(f"Result: {calculator(num1, num2, op)}")
